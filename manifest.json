{
  "manifest_version": 2,
  "name": "business",
  "description": "",
  "version": "1.2.3", // add extraHeaders
  "icons": {
    "16": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  },
  "browser_action": {
    "default_title": "molimediagroup1.0",
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "cookies",
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "downloads",
    "storage",
    "unlimitedStorage",
    "background",
    "*://*.taobao.com/*",
    "https://*.taobao.com/*",
    "http://molitest.willbe.net.cn/*",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": ["js/jquery.min.js", "eventPage.js"]
  },
  "content_scripts": [{
    "matches": ["http://*.molimediagroup.com/*", "https://*.molimediagroup.com/*"],
    "js": ["js/jquery.min.js", "content.js"]
  }]
}
